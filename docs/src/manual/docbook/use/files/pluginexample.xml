<plugin>
  <groupId>org.openengsb.loom.csharp.common</groupId>
  <artifactId>wsdltodll-maven-plugin</artifactId>
  <version>LOOM_CSHARP_COMMON_VERSION</version>
  <configuration>
    <npanday_setting>${user.home}\.m2\npanday-settings.xml</npanday_setting>
    <wsdl_location>${project.build.directory}/DOMAINNAME.wsdl</wsdl_location>
    <namespace>DOMAINNAME</namespace>
  </configuration>
  <executions>
    <execution>
      <phase>compile</phase>
        <goals>
          <goal>run</goal>
        </goals>
    </execution>
  </executions>
</plugin>